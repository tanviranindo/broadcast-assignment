<resources>
    <!-- Base application theme. -->
    <style name="Theme.Broadcastingassignment" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">?android:attr/colorPressedHighlight</item>
        <item name="titleTextColor">@color/headerTitle</item>
        <item name="colorPrimaryDark">?android:attr/colorSecondary</item>
        <item name="colorAccent">?attr/colorPrimary</item>
        <item name="android:fontFamily">@font/poppins_regular</item>
        <!-- Customize your theme here. -->
    </style>
    <style name="Theme.Broadcastingassignment.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>
    <style name="Theme.Broadcastingassignment.AppBarOverlay"
        parent="ThemeOverlay.AppCompat.Dark.ActionBar" />
    <style name="Theme.Broadcastingassignment.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
</resources>